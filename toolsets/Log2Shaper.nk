set N680dde20 [stack 0]
 Expression {
  temp_name0 o_r
  temp_expr0 sign(r)<0?r0*2+add:sign(r)==0?r0+add:add
  temp_name1 o_g
  temp_expr1 sign(g)<0?r0*2+add:sign(g)==0?r0+add:add
