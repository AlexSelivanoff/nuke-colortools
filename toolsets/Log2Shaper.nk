  expr0 fabs(r)<=c?(s*(fabs(r)-c)+o)*sign(r)+o_r:(log(fabs(r)/mid_grey)/log(p)-min_exp)/(max_exp-min_exp)*sign(r)+o_r
  expr1 fabs(g)<=c?(s*(fabs(g)-c)+o)*sign(g)+o_g:(log(fabs(g)/mid_grey)/log(p)-min_exp)/(max_exp-min_exp)*sign(g)+o_g
  expr2 fabs(b)<=c?(s*(fabs(b)-c)+o)*sign(b)+o_b:(log(fabs(b)/mid_grey)/log(p)-min_exp)/(max_exp-min_exp)*sign(b)+o_b
  channel3 none
  name lin_to_log_reflect
  xpos 290
  ypos 86
  addUserKnob {20 User}
  addUserKnob {7 c l cut R 0 0.2}
  c {{max(1e-8,cut)}}
  addUserKnob {7 o l offset}
  o {{(log(c/mid_grey)/log(p)-min_exp)/(max_exp-min_exp)}}
  addUserKnob {7 s l slope}
  s {{1/(log(p)*c*(max_exp-min_exp))}}
  addUserKnob {7 r0 l root t "zero crossing for linear segment"}
  r0 {{-c*s+o}}
  addUserKnob {7 add}
 }
push $N680dde20
 Expression {
